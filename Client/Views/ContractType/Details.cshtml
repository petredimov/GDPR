@model Client.Models.ContractTypeViewModel
@{
    ViewBag.Title = "Contract Management";
}
	<div class="panel panel-info panelBackground h-75 p3">
		<div class="panel-heading"><h3 class="text-dark">Details: @Model.Name</h3></div>
		<div class="panel-body">
			<div>
				<object id='pdfbox' type="application/pdf" data="@Url.Action("GeneratePDF", "ContractType", @Model)"></object>
			</div>
		</div>
	</div>

<style type="text/css">
	#pdfbox {
		width: 100%;
		height: 500px;
		min-height: 100%;
		border: 5px solid #ccc;
	}
</style>







